{
  "name": "Blocking the recommendation out of youtube",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Block the annoying youtube recommendation on home page",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
"browser_action": {
  "default_icon": "theheck.png",
 },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com"],
      "js": ["block.js"]
    }
  ]
}
